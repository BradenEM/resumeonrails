<div id="<%= dom_id(job) %>">
    <%= job.title %><br>
    <%= job.company %><br>
    <%= job.location %><br>
    <%= job.summary %>
    <p>
        <%= link_to "Edit this job", edit_resume_job_path(job.resume,job) %>
    </p>
    <%= button_to "Destroy this job", resume_job_path(job.resume,job), method: :delete %>
</div>