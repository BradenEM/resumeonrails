<p style="color: green"><%= notice %></p>

<% content_for :title, "Resumes" %>

<h1>Resumes</h1>

<div id="resumes">
    <% @resumes.each do |resume| %>
    <%= render resume %>
    <p>
        <%= link_to "Show this resume", resume, class: "link" %>
    </p>
    <% end %>
</div>

<%= link_to "New resume", new_resume_path %>